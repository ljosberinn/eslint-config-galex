      "parserOptions": {
        "ecmaVersion": 2020
      },
      "plugins": [
        "jest"
      ],
      "settings": {
        "jest": {
          "version": "detect"
        }
      },
      "rules": {
        "jest/consistent-test-it": "off",
        "jest/expect-expect": "off",
        "jest/lowercase-name": "off",
        "jest/max-nested-describe": "off",
